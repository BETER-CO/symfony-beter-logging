{
    "name": "beter/symfony-beter-logging",
    "type": "symfony-bundle",
    "description": "Symfony BeterLoggingBundle",
    "keywords": ["log", "logging"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Volkov",
            "email": "d.volkov@beter.co"
        },
        {
            "name": "Rostyslav Toderash",
            "email": "r.toderash@beter.co"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/monolog-bundle": "^3.0",
        "beter/exception-with-context": "^1.0",
        "psr/log": "^3"
    },
    "autoload": {
        "psr-4": { "Beter\\Bundle\\BeterLoggingBundle\\": "src/" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
